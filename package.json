{
    "name": "mamklearn",
    "version": "1.0.0",
    "description": "Mamklearn Project",
    "private": true,
    "type": "module",
    "scripts": {
        "develop": "yarn run parcel ./src/index.html",
        "build": "yarn run check && yarn run parcel build ./src/index.html --no-source-maps",
        "build-dev": "yarn run check && cp dev.firebase.json firebase.json && yarn run parcel build ./src/dev.index.html --no-source-maps",
        "clean": "rm -rf dist && rm -rf .parcel-cache",
        "win-clean": "rmdir /s /q dist  && rmdir /s /q .parcel-cache",
        "check": "yarn run tsc --noEmit && yarn run prettier --check . && yarn run eslint . && yarn run jest --coverage --coverage-reporters=text",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/istrugatskiy/mamklearn.git"
    },
    "author": "Ilya Strugatskiy",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/istrugatskiy/mamklearn/issues"
    },
    "homepage": "https://github.com/istrugatskiy/mamklearn#readme",
    "devDependencies": {
        "@anteris/parcel-transformer-html-literals": "^0.1.0",
        "@parcel/config-default": "^2.8.3",
        "@parcel/core": "^2.8.3",
        "@parcel/resolver-glob": "^2.8.3",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.16.3",
        "@types/parcel-env": "^0.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-lit": "^1.8.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-plugin-tsdoc-require": "^0.0.3",
        "firebase-tools": "^11.29.1",
        "jest": "^29.5.0",
        "parcel": "^2.8.3",
        "prettier": "^2.8.8",
        "process": "^0.11.10",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "4.9.4"
    },
    "dependencies": {
        "@firebase/app": "^0.9.9",
        "eslint-plugin-wc": "^1.5.0",
        "firebase": "9.21.0",
        "jest-environment-jsdom": "^29.5.0",
        "lit": "^2.7.3",
        "tsparticles-engine": "^2.9.3",
        "tsparticles-slim": "^2.9.3"
    },
    "alias": {
        "process": false
    },
    "workspaces": [
        "mamklearn",
        "functions"
    ],
    "packageManager": "yarn@3.5.1"
}
